<div class="col-md-4">
    <div class="card mb-4 shadow-sm" data-id="<%= portfolio_item.id %>">
        <%= image_tag portfolio_img(portfolio_item.thumb_image, 'thumb'), width:'100%' %>
    <div class="card-text">
        <span><%=link_to portfolio_item.title, portfolio_show_path(portfolio_item.id)%></span> <%= portfolio_item.subtitle %>
    </div>
  </div>
    <small class="text-muted">9 mins</small>
</div>
